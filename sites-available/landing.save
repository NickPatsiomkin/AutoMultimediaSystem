server {
    listen 80;
    listen [::]:80;
    server_name patsiomkinhomelab.com;

    return 301 https://$host$request_uri;
}

server {
    listen 80;
    listen [::]:80;

    server_name patsiomkinhomelab.com;

    root /var/www/landing;
    index index.html;

    location / {
        try_files $uri $uri/ =404;
    }
}

